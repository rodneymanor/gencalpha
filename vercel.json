{
  "crons": [
    {
      "path": "/api/cron/rss-update",
      "schedule": "0 6 * * *"
    }
  ],
  "functions": {
    "src/app/api/*/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}